console.log("Print * 4x4 in javaScript"); // print in header

var str = ''; // variable to store output
var numb = 4; // variable for row & column

for(var i=0; i<numb; i++){ // outer loop for row
  for(var j=; j<numb; j++){ // inner loop for column
    str += '*'; // store * in str variable
  }
  str += '\n'; // store new line in str variable
}

console.log(str); // print output through str variable
